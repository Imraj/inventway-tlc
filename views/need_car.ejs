	<body>
		<div id="fh5co-wrapper">
		<div id="fh5co-page">
		<div id="fh5co-header">
			  <% include partials/header %>
		</div>
		<!-- end:fh5co-header -->

		<div id="fh5co-contact-section" style="margin-top:3em;margin-bottom:5em;">
			<div class="container">
				<form action="/car" method="post">
          <div class="row">
            <div class="col-md-6 col-md-offset-3 text-center" >
              <h2>Need a car</h2>

            </div>
          </div>
					<div class="row">

						<div class="col-md-8 col-md-offset-2">

							<div class="row">

								<div class="col-md-12">
									<div class="form-group">
										<select class="form-control" name="shift">
                        <option disabled value="" selected>Select shift type</option>
                        <option>Day Shift</option>
                        <option>Night Shift</option>
                    </select>
									</div>
								</div>

                <div class="col-md-12">
									<div class="form-group">
										<input type="text" class="form-control" name="location" placeholder="Enter location">
									</div>
								</div>

                <div class="col-md-12">
                  <div class="form-group">
                    <input type="text" class="form-control" name="zip_code" placeholder="Zip code">
                  </div>
                </div>

                <div class="col-md-12">
                  <div class="form-group">
                    <textarea class="form-control" id="" cols="30" rows="2" name="experience" placeholder="Experience"></textarea>
                  </div>
                </div>

								<div class="col-md-12">
										<div class="col-md-4">
											<img src="images/img.jpg" />
											<input type="radio" name="car_model" value="img1"/>
										</div>
										<div class="col-md-4">
											<img src="images/img.jpg" />
											<input type="radio" name="car_model" value="img2"/>
										</div>
										<div class="col-md-4">
											<img src="images/img.jpg" />
											<input type="radio" name="car_model" value="img3"/>
										</div>

								</div>
								<div class="col-md-12">
										<div class="col-md-4">
											<img src="images/img.jpg" />
											<input type="radio" name="car_model" value="img4"/>
										</div>
										<div class="col-md-4">
											<img src="images/img.jpg" />
											<input type="radio" name="car_model" value="img5"/>
										</div>
										<div class="col-md-4">
											<img src="images/img.jpg" />
											<input type="radio" name="car_model" value="img6"/>
										</div>

								</div>

								<div class="col-md-12">
									<div class="form-group">
										<select class="form-control" name="car_year">
												<option disabled value="" selected>Select model year</option>
												<option>2000</option>
												<option>2001</option>
												<option>2002</option>
												<option>2003</option>
												<option>2004</option>
												<option>2005</option>
												<option>2006</option>
												<option>2007</option>
												<option>2008</option>
												<option>2009</option>
												<option>2010</option>
										</select>
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group">
										<input type="submit" value="Submit" class="btn btn-primary col-md-12">
									</div>
								</div>
							</div>
						</div>
					</div>
				</form>
			</div>
		</div>

	  <% include partials/footer %>


	</div>
